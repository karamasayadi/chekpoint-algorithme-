algorithme chekpoint 
var 
i:integer;
mot:integer;
car:integer;
voy:integer;
x:string;
begin 
read (x);
for (i from 1 to longeur(x)step i++)do 
car=car+1;
if (x[i]="") then 
mot =mot +1  ;
else if (x[i]="a" || x[i]="A")
voy =voy +1;
else if (x[i]="e" || x[i]="E")
voy =voy +1;
else if (x[i]="i" || x[i]="I")
voy =voy +1;
else if (x[i]="o" || x[i]="O")
voy =voy +1;
else if (x[i]="u" || x[i]="U")
voy =voy +1;
else if (x[i]="y" || x[i]="Y")
voy =voy +1;
end if 
end for 
end .

